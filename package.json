{
  "name": "sorsio-3d",
  "version": "0.1.0",
  "description": "Interactive educational application for teaching and exploring the Tarot de Marseille card game",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "gsap": "^3.14.2",
    "three": "0.182.0",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.7",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "@types/three": "^0.182.0",
    "sass": "^1.97.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "vue-eslint-parser": "^10.2.0",
    "typescript-eslint": "^8.50.1",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "typescript": "~5.8.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  }
}
